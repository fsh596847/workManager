package com.orwlw.model;

import java.io.Serializable;

public class CheckItem implements Serializable {

	/**
	 * 检查项实体
	 */
	private static final long serialVersionUID = 1477426878217491955L;

	private String id;
	private String type;
	private boolean F_PHOTO_NEED;
	private boolean F_COMM_LINKED;
	private boolean F_COMM_PHOTO_REALATION;
	public String getId() {
		return id;
	}
	public void setId(String id) {
		this.id = id;
	}
	public String getType() {
		return type;
	}
	public void setType(String type) {
		this.type = type;
	}
	public boolean isF_PHOTO_NEED() {
		return F_PHOTO_NEED;
	}
	public void setF_PHOTO_NEED(boolean f_PHOTO_NEED) {
		F_PHOTO_NEED = f_PHOTO_NEED;
	}
	public boolean isF_COMM_LINKED() {
		return F_COMM_LINKED;
	}
	public void setF_COMM_LINKED(boolean f_COMM_LINKED) {
		F_COMM_LINKED = f_COMM_LINKED;
	}
	public boolean isF_COMM_PHOTO_REALATION() {
		return F_COMM_PHOTO_REALATION;
	}
	public void setF_COMM_PHOTO_REALATION(boolean f_COMM_PHOTO_REALATION) {
		F_COMM_PHOTO_REALATION = f_COMM_PHOTO_REALATION;
	}
	
	

}
