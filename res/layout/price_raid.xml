<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="horizontal" android:layout_width="fill_parent"
	android:layout_height="wrap_content" android:gravity="center"
	android:layout_marginLeft="20dip" android:layout_marginRight="20dip">
	<TextView android:id="@+id/F_type_ID" android:visibility="gone"
		android:layout_width="wrap_content" android:layout_height="fill_parent"
		android:textSize="16dp" android:gravity="center_vertical"
		android:paddingLeft="10dp" android:height="40dip" />
	<TextView android:id="@+id/F_type_name" android:text="价格级别"
		android:layout_width="wrap_content" android:layout_height="fill_parent"
		android:textSize="16dp" android:gravity="center_vertical"
		android:paddingLeft="10dp" android:height="40dip" />
</LinearLayout>