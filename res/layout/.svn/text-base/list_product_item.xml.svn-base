<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:id="@+id/layout_myexam_item" android:layout_width="fill_parent"
	android:layout_height="wrap_content" android:background="@drawable/bg_list_item"
	android:orientation="vertical">


	<LinearLayout android:layout_width="fill_parent"
		android:layout_height="wrap_content" android:layout_centerInParent="true"
		android:layout_marginRight="20dp" android:layout_toRightOf="@+id/image_myExam_infoIcon"
		android:orientation="vertical">
	
		<TextView android:id="@+id/list_comm_code" android:visibility="gone"
			android:layout_margin="10dp" android:layout_width="fill_parent"
			android:layout_height="wrap_content" android:text="00001"
			android:textColor="@color/black" android:textSize="15dp" />
		<TextView android:id="@+id/list_comm_name"
			android:layout_margin="10dp" android:layout_width="fill_parent"
			android:layout_height="wrap_content" android:text="QQ星儿童奶原味200ml"
			android:textColor="@color/black" android:textSize="15dp" />
	</LinearLayout>

	<ImageView android:id="@+id/goorder" android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:layout_alignParentRight="true"
		android:layout_centerVertical="true" android:layout_marginRight="10dp"
		android:src="@drawable/goorder" />

</RelativeLayout>